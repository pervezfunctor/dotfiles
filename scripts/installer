#! /usr/bin/env bash

{

command -v curl > /dev/null || {
    echo "curl not found. Please install curl and try again."
    exit 1
}

source <(curl -sSL https://dub.sh/z3pTnAK)

main() {
    while [[ "$#" -gt 0 ]]; do
    if fn_exists "${1}_install"; then
        "${1}_install"
    else
        err_exit "No such installer: $1"
    fi
    shift
    done
}

bootstrap "$@"

}
